{
  "name": "mayu-ui",
  "version": "0.14.4",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joburgard/mayu-ui.git"
  },
  "homepage": "https://github.com/joburgard/mayu-ui",
  "dependencies": {
    "@iconify-json/svg-spinners": "^1.2.1",
    "@leeoniya/ufuzzy": "^1.0.14",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@commitlint/cz-commitlint": "^18.6.1",
    "@iconify-json/lucide": "^1.2.10",
    "@storybook/addon-essentials": "^8.3.6",
    "@storybook/addon-interactions": "^8.3.6",
    "@storybook/addon-links": "^8.3.6",
    "@storybook/addon-svelte-csf": "4.1.7",
    "@storybook/blocks": "^8.3.6",
    "@storybook/manager-api": "^8.3.6",
    "@storybook/svelte": "^8.3.6",
    "@storybook/sveltekit": "^8.3.6",
    "@storybook/test": "^8.3.6",
    "@storybook/theming": "^8.3.6",
    "@sveltejs/adapter-auto": "^3.3.0",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/package": "^2.3.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "autoprefixer": "^10.4.20",
    "commit-and-tag-version": "^12.5.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.10.1",
    "eslint-plugin-svelte": "^2.46.0",
    "husky": "^9.1.6",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "publint": "^0.2.12",
    "storybook": "^8.3.6",
    "svelte-check": "^4.0.5",
    "svelte-sequential-preprocessor": "^2.0.2",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  },
  "peerDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.83.0",
    "@unocss/extractor-svelte": "^0.62.3",
    "@unocss/reset": "^0.62.3",
    "svelte": "^4.0.0",
    "tailwind-variants": "^0.2.1",
    "unocss": "^0.62.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "license": "Unlicense",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "package:watch": "svelte-kit sync && svelte-package --watch && publint",
    "prepublishOnly": "npm run package",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "storybook": "storybook dev -p 6006",
    "storybook-docs": "storybook dev --docs",
    "build-storybook": "storybook build",
    "build-storybook-docs": "storybook build --docs",
    "release": "commit-and-tag-version -a",
    "cm": "git-cz",
    "commitlint": "commitlint --edit",
    "prepare": "husky"
  },
  "type": "module",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "commit-and-tag-version": {
    "scripts": {
      "postchangelog": "cat ./CHANGELOG.md > ./src/stories/Changelog.mdx && git add ./src/stories/Changelog.mdx"
    }
  }
}
